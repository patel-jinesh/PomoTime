    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-svelte3": "^3.2.1",
    "jest": "^27.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.46.3",
    "svelte-check": "^2.2.6",
    "svelte-jester": "^2.0.1",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.9.4",
    "ts-jest": "^27.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
